
public class Solution {

	public static boolean isPalindrome(String str) {
		//Your code goes here
        int s =0, e= str.length()-1;
        while(s+){
            if( str.charAt(s)==str.charAt(e))
                {
                    s++;
                    e--;
                } 
        else{
            return false;
        } 
    }
        return true;
       
	}

}