
public class Solution {  

    public static void insertionSort(int[] arr) {
    	//Your code goes here
        int n = arr.length;
        int j=1;
        while(j<n){
            
            int i = j-1;
            int temp= arr[j];
            while(i>=0 && arr[i]>temp){
                arr[i+1]= arr[i];
    			i--;
            }
            
            arr[i+1]= temp;
            j++;
        }
            
    }
      

}
