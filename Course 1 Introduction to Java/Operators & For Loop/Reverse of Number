import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
Scanner s= new Scanner( System.in);
        int n =s.nextInt();
        int sum= 0;
        
        while(n>0){
        
        int l = n%10;
        sum= sum*10+l;
        n= n/10;
            
        }
        System.out.print(sum);
	}
}
