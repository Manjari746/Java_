import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
int no, sum1=0, sum2=0;
        Scanner s= new Scanner(System.in);
        no= s.nextInt();
        while(no>0){
            int i= no%10;
            if(i%2==0){
                sum1=sum1+i;
            }
            else{
                sum2=sum2+i;
            }
            no=no/10;
        }
      System.out.println(sum1+" "+sum2);  
	}
}
