import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
		//Your code goes here
		Scanner s= new Scanner (System.in);
        int n= s.nextInt();
        
        for (int i= 1;i<=n;i++)
        {
           
            for(int j=0; j<=i-1; j++)
            {
             System.out.print((char)('A'+n-i+j));

            }
            System.out.println();
        }
    }
}
