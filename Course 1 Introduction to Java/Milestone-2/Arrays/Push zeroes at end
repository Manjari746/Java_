public class Solution {  

    public static void pushZerosAtEnd(int[] arr) {
    	//Your code goes here
        int n =arr.length;
        int i =0;
        for( int j=0; j<n; j++){
            
            if( arr[j]!=0){
               arr[i]=arr[j];
             
                i++;
            }
            
        }
        
        while(i<n){
            arr[i]=0;
            i++;
        }
    }

}
