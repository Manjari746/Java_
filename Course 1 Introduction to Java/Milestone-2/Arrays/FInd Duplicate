
public class Solution{  
    
    public static int duplicateNumber(int arr[]) {
    	//Your code goes here
        int n= arr.length;
        //boolean isDuplicate= false;
        for (int i =0; i<n ; i++){
            for( int j =0; j<n; j++){
                if( i!=j && arr[i]==arr[j]){
                    return arr[i];
                }
                
            }
        }
        return 0;
    }
}
